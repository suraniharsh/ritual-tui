# SonarQube Configuration
# Copy this file to .env and fill in the values

# SonarQube Server URL
SONAR_HOST_URL=http://localhost:9000

# SonarQube Authentication Token
# Generate at http://localhost:9000/account/security
SONAR_TOKEN=your_token_here

# SonarQube Project Key (must match your project key in SonarQube)
SONAR_PROJECT_KEY=ritual-local

# Optional: Database credentials (default values used in docker-compose.yaml)
POSTGRES_USER=sonar
POSTGRES_PASSWORD=sonar
POSTGRES_DB=sonar
POSTGRES_PORT=5432  # Internal port (mapped to 5003 on host)

